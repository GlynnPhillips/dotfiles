[credential]
	helper = osxkeychain
[core]
	excludesfile = ~/.dotfiles/.gitignore
	editor = /usr/bin/vim
[color]
	ui = auto
[alias]
	st = status
	co = checkout
	ci = commit
[branch]
	autosetupmerge = true
	autosetuprebase = always
[diff]
	algorithm = patience
[push]
	default = simple
[user]
	name = Glynn Phillips
	email = glynndominicphillips@gmail.com
